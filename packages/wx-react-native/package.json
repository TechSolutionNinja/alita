{
  "name": "@areslabs/wx-react-native",
  "version": "2.3.6",
  "description": "微信版本的React Native",
  "files": [
    "dist",
    "package.json",
    "wxComponents"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c"
  },
  "author": "",
  "license": "MIT",
  "wxComponents": {
    "path": "wxComponents",
    "components": [
      {
        "name": "WXButton",
        "path": "WXButton/index"
      },
      {
        "name": "WXFlatList",
        "path": "WXFlatList/index",
        "base": false,
        "jsxProps": {
          "ListHeaderComponent": "ListHeaderComponentCPT",
          "ListFooterComponent": "ListFooterComponentCPT",
          "ListEmptyComponent": "ListEmptyComponentCPT",
          "renderItem": "renderItemCPT",
          "ItemSeparatorComponent": "ItemSeparatorCompoCPT"
        }
      },
      {
        "name": "WXModal",
        "path": "WXModal/index"
      },
      {
        "name": "WXPicker",
        "path": "WXPicker/index",
        "needOperateChildren": true,
        "base": false
      },
      {
        "name": "WXPickerItem",
        "path": "WXPickerItem/index"
      },
      {
        "name": "WXScrollView",
        "path": "WXScrollView/index"
      },
      {
        "name": "WXSlider",
        "path": "WXSlider/index"
      },
      {
        "name": "WXSwitch",
        "path": "WXSwitch/index"
      },
      {
        "name": "WXSectionList",
        "path": "WXSectionList/index",
        "base": false,
        "jsxProps": {
          "renderSectionHeader": "renderSectionHeaderCPT",
          "renderSectionFooter": "renderSectionFooterCPT",
          "renderItem": "renderItemCPT",
          "ListHeaderComponent": "ListHeaderComponentCPT",
          "ListFooterComponent": "ListFooterComponentCPT",
          "ListEmptyComponent": "ListEmptyComponentCPT",
          "SectionSeparatorComponent": "SectionSeparatorCoCPT"
        }
      },
      {
        "name": "WXTextInput",
        "path": "WXTextInput/index"
      },
      {
        "name": "WXActivityIndicator",
        "path": "WXActivityIndicator/index"
      },
      {
        "name": "WXImageBackground",
        "path": "WXImageBackground/index"
      }
    ]
  }
}
