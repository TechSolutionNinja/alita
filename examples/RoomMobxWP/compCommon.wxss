/*TouchableOpacity TouchableHighlight 使用*/
.thFabricate {
    display: none;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

.thFirst {
    z-index: 1;
}

.thHover > .thFabricate {
    display: block;
}

.touchableOpacity0 {
    opacity: 0
}

.touchableOpacity1 {
    opacity: 0.1
}

.touchableOpacity2 {
    opacity: 0.2
}

.touchableOpacity3 {
    opacity: 0.3
}

.touchableOpacity4 {
    opacity: 0.4
}

.touchableOpacity5 {
    opacity: 0.5
}

.touchableOpacity6 {
    opacity: 0.6
}

.touchableOpacity7 {
    opacity: 0.7
}

.touchableOpacity8 {
    opacity: 0.8
}

.touchableOpacity9 {
    opacity: 0.9
}

.touchableOpacity10 {
    opacity: 1
}
